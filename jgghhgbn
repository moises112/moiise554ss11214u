			var var1=document.getElementById('PRECIO3').value;
			var var2=document.getElementById('CANTIDAD3').value;
			var res3=var1*var2;
			document.getElementById('N3').value=res3;

			var var1=document.getElementById('PRECIO4').value;
			var var2=document.getElementById('CANTIDAD4').value;
			var res4=var1*var2;
			document.getElementById('N4').value=res4;

			var var1=document.getElementById('PRECIO5').value;
			var var2=document.getElementById('CANTIDAD5').value;
			var res5=var1*var2;
			document.getElementById('N5').value=res5;


			document.getElementById('N6').value=(res+res2+res3+res4+res5);
            		
		}
